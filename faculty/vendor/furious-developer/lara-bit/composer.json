{
    "name": "furious-developer/lara-bit",
    "description": "Create Laravel views (blade template) using 'php artisan' command-line interface",
    "type": "library",
    "keywords": [
        "laravel",
        "views",
        "blade",
        "template",
        "generator",
        "php",
        "artisan",
        "command"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ragib Mrb",
            "email": "ragib.mrb@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "FuriousDeveloper\\LaraBit\\": "src/"
        }
    },
    "extra": {
        "laravel":{
            "providers":[
                "FuriousDeveloper\\LaraBit\\LaraBitServiceProvider"
            ]
        }
    }
}